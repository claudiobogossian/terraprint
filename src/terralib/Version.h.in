/*!
  \file terralib/terraprint_version.h

  \brief System versioning macros.
*/

#ifndef __TERRAPRINT_INTERNAL_TERRAPRINT_VERSION_H__
#define __TERRAPRINT_INTERNAL_TERRAPRINT_VERSION_H__

/*!
  \def TERRALIB_LAYOUT_VERSION_MAJOR

  \brief Major version of TerraPrint (or version).
 */
#define TERRALIB_LAYOUT_VERSION_MAJOR @TERRALIB_LAYOUT_VERSION_MAJOR@

/*!
  \def TERRALIB_LAYOUT_VERSION_MINOR

  \brief Minor version of TerraPrint (or revision). 
 */
#define TERRALIB_LAYOUT_VERSION_MINOR @TERRALIB_LAYOUT_VERSION_MINOR@

/*!
  \def TERRALIB_LAYOUT_VERSION_PATCH

  \brief Patched version of TerraPrint (or patch).
 */
#define TERRALIB_LAYOUT_VERSION_PATCH @TERRALIB_LAYOUT_VERSION_PATCH@

/*!
  \def TERRALIB_LAYOUT_VERSION_STRING

  \brief This flag is used for versioning the TerraPrint code. If you have plugins and other tools
         that must check the TerraPrint version, you can test against this string.
 */
#define TERRALIB_LAYOUT_VERSION_STRING "@TERRALIB_LAYOUT_VERSION_STRING@"

/*!
  \def TERRALIB_LAYOUT_VERSION

  \brief This flag is used for versioning the TerraPrint code. If you have plugins and other tools
         that must check the TerraPrint version, you can test against this number.
 */
#define TERRALIB_LAYOUT_VERSION @TERRALIB_VERSION@

#endif  // __TERRAPRINT_INTERNAL_TERRAPRINT_VERSION_H__
